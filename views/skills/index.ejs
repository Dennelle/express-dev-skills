<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Developer Skills</title>
</head>
<body>
    <h1>Developer Skills</h1>
    <a href ='/skills'>Dev Skills List</a>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
          <a href="/skills/<%= t.id%>"><%= t.Skill %></a>
            -
          <%= t.skills ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
  </body>
</html>
